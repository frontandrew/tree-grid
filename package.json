{
  "name": "tree-grid",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm ci && husky",
    "dev": "vite --host --open",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview --host --open",
    "test": "vitest",
    "lint": "eslint --ext .js,.ts,.vue --fix src",
    "format": "prettier . --write"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "npx eslint",
      "npx prettier --write"
    ],
    "*.{css,html,json,md}": "npx prettier --write"
  },
  "dependencies": {
    "ag-grid-enterprise": "^34.3.1",
    "ag-grid-vue3": "^34.3.1",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
